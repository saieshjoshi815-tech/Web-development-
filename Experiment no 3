A) ORDERED LIST 
#include <iostream>
using namespace std;

int main() {
    int arr[5] = {10, 20, 30, 40, 50};

    for(int i = 0; i < 5; i++) {
        cout << arr[i] << " ";
    }
    return 0;
}
B) UNORDERED LIST 
#include <iostream>
#include <unordered_set>
using namespace std;

int main() {
    unordered_set<int> s = {10, 40, 20, 30};

    for(int x : s) {
        cout << x << " ";
    }
    return 0;
}
